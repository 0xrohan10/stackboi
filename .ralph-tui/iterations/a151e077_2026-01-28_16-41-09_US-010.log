}</Text>\n  1063→    </Box>\n  1064→  );\n  1065→}\n  1066→\n  1067→// Sync progress dismiss handler component\n  1068→function SyncProgressWithDismiss({\n  1069→  progress,\n  1070→  onDismiss,\n  1071→}: {\n  1072→  progress: SyncProgress;\n  1073→  onDismiss: () => void;\n  1074→}) {\n  1075→  useInput(() => {\n  1076→    if (progress.state === \"success\" || progress.state === \"error\") {\n  1077→      onDismiss();\n  1078→    }\n  1079→  });\n  1080→\n  1081→  return <SyncProgressOverlay progress={progress} />;\n  1082→}\n  1083→\n  1084→// Main app component\n  1085→function App() {\n  1086→  const { exit } = useApp();\n  1087→  const [loading, setLoading] = useState(true);\n  1088→  const [error, setError] = useState<string | null>(null);\n  1089→  const [stacks, setStacks] = useState<StackWithInfo[]>([]);\n  1090→  const [currentBranch, setCurrentBranch] = useState<string>(\"\");\n  1091→  const [checkingOut, setCheckingOut] = useState<string | null>(null);\n  1092→  const [ghAuthenticated, setGhAuthenticated] = useState(false);\n  1093→  const [pollIntervalMs, setPollIntervalMs] = useState(DEFAULT_POLL_INTERVAL_MS);\n  1094→  const [isPolling, setIsPolling] = useState(false);\n  1095→  const [mergeNotification, setMergeNotification] = useState<MergedPRNotification | null>(null);\n  1096→  const [syncProgress, setSyncProgress] = useState<SyncProgress | null>(null);\n  1097→  const [rerereStats, setRerereStats] = useState<RerereStats | null>(null);\n  1098→  const [conflictState, setConflictState] = useState<ConflictState | null>(null);\n  1099→  const [pendingNotification, setPendingNotification] = useState<MergedPRNotification | null>(null);\n  1100→\n  1101→  // Initial load\n  1102→  useEffect(() => {\n  1103→    async function load() {\n  1104→      try {\n  1105→        const [branch, gitRoot, authenticated, stats] = await Promise.all([\n  1106→          getCurrentBranch(),\n  1107→          getGitRoot(),\n  1108→          checkGhAuth(),\n  1109→      